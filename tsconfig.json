{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": false,
    "baseUrl": ".",
    "typeRoots": ["app/types"],
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"],
      "~/assets": ["app/assets"],
      "~/components": ["app/components"],
      "~/configs": ["app/configs"],
      "~/consts": ["app/consts"],
      "~/hooks": ["app/hooks"],
      "~/i18n": ["app/i18n"],
      "~/navigation": ["app/navigation"],
      "~/screens": ["app/screens"],
      "~/services": ["app/services"],
      "~/styles": ["app/styles"],
      "~/types": ["app/types"],
      "~/utils": ["app/utils"],
      "~/redux": ["app/redux"]
    }
  }
}
